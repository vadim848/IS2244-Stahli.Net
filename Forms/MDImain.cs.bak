        private void _tbr_button_Click(object sender, EventArgs e)
        {
            ToolStripButton toolStripButton = sender as ToolStripButton; 

             switch (toolStripButton.Name)
            {
                case "_tbr_Main":
                    break;
                case "_tbr_Conv":                    
                    frmAssemblies.Show();
                    break;
                case "_tbr_Robot":
                    frmRobots.Show();
                    break;
                case "_tbr_Camera":
                    if (chkPlcON.Checked)
                    {
                        frmVisionMain.Show();
                    }
                    break;
                case "_tbr_Tray":
                    break;
                case "_tbr_Carr":
                    break;
                case "_tbr_Order":
                    frmOrderEditor.Show();
                    break;
                case "_tbr_PLC":
                    frmBeckhoff.Show();
                    break;
                case "_tbr_ES_Rel":
                    break;
                case "_tbr_Exit":
                    this.Close();
                    break;
                default:
                    break;
            }
        }
     
